doctype html
html(lang="en")
    head
        meta(charset="utf-8")
        title Adventure Bootstrap Theme by tooplate
        meta(http-equiv="X-UA-Compatible" content="IE=Edge")
        meta(name="viewport" content="width=device-width, initial-scale=1")
        meta(name="keywords" content="")
        meta(name="description" content="")
        block styles
            //Bootstrap CSS
            link(rel="stylesheet" href="css/bootstrap.min.css")
            //Animate CSS
            link(rel="stylesheet" href="css/animate.min.css")
            //Font Icons
            link(rel="stylesheet" href="css/font-awesome.min.css")
            link(rel="stylesheet" href="css/et-line-font.css")
            //Nivo Lightbox CSS
            link(rel="stylesheet" href="css/nivo-lightbox.css")
            link(rel="stylesheet" href="css/default.css")
            //Owl Carousel CSS
            link(rel="stylesheet" href="css/owl.theme.css")
            link(rel="stylesheet" href="css/owl.carousel.css")
            //BxSlider CSS
            link(rel="stylesheet" href="css/bxslider.css")
            //Google Web Font
            link(rel="stylesheet" href="https://fonts.googleapis.com/css?family=Raleway:700")
    body(data-spy="scroll" data-target=".navbar-collapse" data-offset="50")
        include component/preloader
        include layout/header
        block content
        include layout/footer
        block scripts
            script(src="js/jquery.js")
            script(src="js/bootstrap.min.js")
            script(src="js/smoothscroll.js")
            script(src="js/nivo-lightbox.min.js")
            script(src="js/jquery.easing-1.3.js")
            script(src="js/plugins.js")
            script(src="js/owl.carousel.min.js")
            script(src="js/jquery.parallax.js")
            script(src="js/wow.min.js")


